{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "required": [
    "volume",
    "symbolsTraded",
    "routedVolume",
    "notional"
  ],
  "properties": {
    "volume": {
      "$id": "#/properties/volume",
      "type": "object",
      "required": [
        "recordValue",
        "recordDate",
        "previousDayValue",
        "avg30Value"
      ],
      "properties": {
        "recordValue": {
          "$id": "#/properties/volume/properties/recordValue",
          "type": "number"
        },
        "recordDate": {
          "$id": "#/properties/volume/properties/recordDate",
          "type": "string",
          "pattern": "^(.*)$"
        },
        "previousDayValue": {
          "$id": "#/properties/volume/properties/previousDayValue",
          "type": "number"
        },
        "avg30Value": {
          "$id": "#/properties/volume/properties/avg30Value",
          "type": "number"
        }
      }
    },
    "symbolsTraded": {
      "$id": "#/properties/symbolsTraded",
      "type": "object",
      "required": [
        "recordValue",
        "recordDate",
        "previousDayValue",
        "avg30Value"
      ],
      "properties": {
        "recordValue": {
          "$id": "#/properties/symbolsTraded/properties/recordValue",
          "type": "number"
        },
        "recordDate": {
          "$id": "#/properties/symbolsTraded/properties/recordDate",
          "type": "string",
          "pattern": "^(.*)$"
        },
        "previousDayValue": {
          "$id": "#/properties/symbolsTraded/properties/previousDayValue",
          "type": "number"
        },
        "avg30Value": {
          "$id": "#/properties/symbolsTraded/properties/avg30Value",
          "type": "number"
        }
      }
    },
    "routedVolume": {
      "$id": "#/properties/routedVolume",
      "type": "object",
      "required": [
        "recordValue",
        "recordDate",
        "previousDayValue",
        "avg30Value"
      ],
      "properties": {
        "recordValue": {
          "$id": "#/properties/routedVolume/properties/recordValue",
          "type": "number"
        },
        "recordDate": {
          "$id": "#/properties/routedVolume/properties/recordDate",
          "type": "string",
          "pattern": "^(.*)$"
        },
        "previousDayValue": {
          "$id": "#/properties/routedVolume/properties/previousDayValue",
          "type": "number"
        },
        "avg30Value": {
          "$id": "#/properties/routedVolume/properties/avg30Value",
          "type": "number"
        }
      }
    },
    "notional": {
      "$id": "#/properties/notional",
      "type": "object",
      "required": [
        "recordValue",
        "recordDate",
        "previousDayValue",
        "avg30Value"
      ],
      "properties": {
        "recordValue": {
          "$id": "#/properties/notional/properties/recordValue",
          "type": "number"
        },
        "recordDate": {
          "$id": "#/properties/notional/properties/recordDate",
          "type": "string",
          "pattern": "^(.*)$"
        },
        "previousDayValue": {
          "$id": "#/properties/notional/properties/previousDayValue",
          "type": "number"
        },
        "avg30Value": {
          "$id": "#/properties/notional/properties/avg30Value",
          "type": "number"
        }
      }
    }
  }
}
